{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:recommended"],
	"rangeStrategy": "bump",
	"packageRules": [
		{
			"matchDepTypes": ["peerDependencies"],
			"enabled": false
		},
		{
			"matchPackageNames": ["@iconify/utils"],
			"allowedVersions": "<3.0.0"
		},
		{
			"matchPackageNames": ["unbuild"],
			"allowedVersions": "<3.0.0"
		}
	]
}
